procedure spokes(x, y, r, i, angle)
   local incr, j

   incr := 2 * &pi / i 

   every j := 1 to i do {
      DrawLine(x, y, x + r * cos(angle), y + r * sin(angle))
      angle +:= incr
      }

   return

end

