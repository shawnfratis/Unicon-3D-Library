############################################################################
#
#  This program animates various shapes by moving the mouse.
#  NOTE: This doesn't use OpenGL.  
#
############################################################################

link wopen
link graphics

procedure main()

WOpen("label=no GL shapes","width=600","height=300","fg=yellow","bg=black")| stop("*** Fail")

WAttrib("dx="||300, "dy="||150)

WWrite("Press and drag left mouse button to get things moving!")

main3()

end

procedure main3()

circle()

repeat {

while back(ax,ay) do {

if ax <:= 500 then
calc()

else

return 

}
}

end

procedure circle()

x := 1
y := 1
r := 100

Fg("yellow")

DrawCircle(x,y,r)

end

######################################################

procedure back(ax,ay)

repeat {

case Event() of {
&ldrag:{

animatedModule01(&x,&y,&x)

WDelay(15)

EraseArea()

circle()

}
}
}

end

